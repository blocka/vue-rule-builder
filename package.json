{
  "name": "vue-rule-builder",
  "version": "1.0.3",
  "description": "A vue component for building complex filtering rules",
  "scripts": {
    "test": "jest --verbose --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "bili src/index.js src/components/Builder.js --plugin vue --js babel --no-babel.babelrc --format cjs,umd,umd-min,es  && cp src/assets/styles.css dist/styles.css",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/vue-rule-builder.es.js",
  "unpkg": "dist/vue-rule-builder.min.js",
  "jsdelivr": "dist/vue-rule-builder.min.js",
  "files": [
    "dist/*.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blocka/vue-rule-builder.git"
  },
  "keywords": [
    "vue",
    "scoped-slot",
    "rule-builder"
  ],
  "author": "Avi Block <atblock@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blocka/vue-rule-builder/issues"
  },
  "homepage": "https://github.com/blocka/vue-rule-builder#readme",
  "dependencies": {
    "normalizr": "^3.2.4",
    "omit-deep-lodash": "^1.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.16"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addon-notes": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/storybook-deployer": "^2.3.0",
    "@storybook/vue": "^3.4.8",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-vue": "^2.0.2",
    "babel-preset-vue-app": "^2.0.0",
    "bili": "^3.1.2",
    "css-loader": "^0.28.11",
    "jest": "^23.2.0",
    "prettier": "^1.13.7",
    "rollup-plugin-vue": "^2.5.4",
    "vue-jest": "^2.6.0",
    "vue-loader": "^14.2.3"
  }
}
